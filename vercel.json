{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    }
  ],
  "build": {
    "env": {
      "NODE_ENV": "production"
    }
  },
  "scripts": {
    "postinstall": "npm run build:css"
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.js"
    }
  ]
}
